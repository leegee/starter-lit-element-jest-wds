{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true,
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unexpected-multiline": "off"
  }
}