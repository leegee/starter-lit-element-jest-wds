{
  "name": "lit-element-starter-js",
  "version": "0.0.0",
  "description": "A simple web component",
  "main": "my-element.js",
  "module": "my-element.js",
  "type": "module",
  "scripts": {
    "lint": "lit-analyzer my-element.js && eslint '**/*.js'",
    "format": "prettier my-element.js test/* --write",
    "analyze": "wca analyze \"**/*.js\" --outFile custom-elements.json",
    "serve": "web-dev-server --app-index dev/index.html --node-resolve --watch --open",
    "test": "jest",
    "checksize": "rollup -c ; cat my-element.bundled.js | gzip -9 | wc -c ; rm my-element.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "lit-element": "^2.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@web/dev-server": "^0.1.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "lit-analyzer": "^1.1.9",
    "prettier": "^2.0.4",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "web-component-analyzer": "^1.0.3"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
